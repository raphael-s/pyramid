<!DOCTYPE html>
<html>
<head>${panel('header')}</head>
<body class="main-content">
    ${panel('navigation')}
    <div class="row image-listing">
        <div class="col-md-8 col-md-offset-2">
            <div tal:repeat="item items" class="list-group">
                <a href="${request.route_url('images', id=item['id'])}" class="list-group-item">
                    <div class="list-image">
                        <div class="row">
                            <div class="col-md-5">
                                <img tal:attributes="src string:data:image;;base64,${item['image']}" class="listed-image">
                            </div>
                            <div class="col-md-7">
                                <h1 tal:content="item['title']">Title</h1>
                                <p tal:content="item['desc']">Description</p>
                            </div>
                        </div>
                    </div>
                </a>
            </div>
        </div>
    </div>
</body>
</html>
